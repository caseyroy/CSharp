@model int[];
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All the Numbers</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <h3>Here are some Numbers!</h3>
    <div>
        @foreach(var num in Model)
        {
            <p>@num</p>            
        }

    </div>
    
</body>
</html>